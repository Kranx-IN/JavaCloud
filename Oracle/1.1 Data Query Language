//Assignments
// (1.1) Data Query Language

1.select staff_name as "Staff Name",design_code as "Designation Code" from staff_master where hiredate < '01-Jan-2003' and staff_sal between 12000 and 25000;

2.select staff_name as "Staff Name", design_code as "Designation Code" from staff_master,hiredate where round(months_between(sysdate,hiredate)/12) >= 18 order by hiredate;

